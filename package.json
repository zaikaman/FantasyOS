{
  "name": "enchanted-realm-shell",
  "version": "0.1.0",
  "description": "A fantasy-themed desktop environment simulator in the browser",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,css,html}\""
  },
  "keywords": [
    "fantasy",
    "desktop-environment",
    "browser-os",
    "sql.js",
    "vite",
    "particles",
    "window-manager"
  ],
  "author": "RuneShell Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.44.0",
    "vite": "^5.4.21",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "Chrome >= 90",
    "Firefox >= 88",
    "Safari >= 14"
  ],
  "dependencies": {
    "@vapi-ai/web": "^2.5.0",
    "sql.js": "^1.13.0"
  }
}
